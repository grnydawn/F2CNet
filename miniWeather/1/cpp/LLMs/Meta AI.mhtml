terf). Dimensions: (1:nz+1)
real(rp), allocatable :: hy_dens_theta_int (:) !hydrostatic rho*t (vert cel=
l interf). Dimensions: (1:nz+1)
real(rp), allocatable :: hy_pressure_int (:) !hydrostatic press (vert cell =
interf). Dimensions: (1:nz+1)

