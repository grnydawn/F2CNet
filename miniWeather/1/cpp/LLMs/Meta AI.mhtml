!The x-direction length is twice as long as the z-direction length
!So, you'll want to have nx_glob be twice as large as nz_glob
integer , parameter :: nx_glob =3D _NX !Number of total grid cells in the x=
 dimension
integer , parameter :: nz_glob =3D _NZ !Number of total grid cells in the z=
 dimension
real(rp), parameter :: sim_time =3D _SIM_TIME !How many seconds to run the =
simulation
real(rp), parameter :: output_freq =3D _OUT_FREQ !How frequently to output =
data to file (in seconds)
integer , parameter :: data_spec_int =3D _DATA_SPEC !How to initialize the =
data
!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!=
