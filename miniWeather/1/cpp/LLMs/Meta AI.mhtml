real(rp) :: dt !Model time step (seconds)
integer :: nx, nz !Number of local grid cells in the x- and z- dimensions f=
or this MPI task
real(rp) :: dx, dz !Grid space length in x- and z-dimension (meters)
integer :: i_beg, k_beg !beginning index in the x- and z-directions for thi=
s MPI task
integer :: nranks, myrank !Number of MPI ranks and my rank id
integer :: left_rank, right_rank !MPI Rank IDs that exist to my left and ri=
ght in the global domain
logical :: mainproc !Am I the main process (rank =3D=3D 0)?
real(rp), allocatable :: hy_dens_cell (:) !hydrostatic density (vert cell a=
vgs). Dimensions: (1-hs:nz+hs)
real(rp), allocatable :: hy_dens_theta_cell(:) !hydrostatic rho*t (vert cel=
l avgs). Dimensions: (1-hs:nz+hs)
