program miniweather
use mpi
implicit none
!Declare the precision for the real constants (at least 15 digits of accura=
cy =3D double precision)
#ifdef SINGLE_PREC
integer , parameter :: rp =3D selected_real_kind(6)
integer , parameter :: mpi_type =3D MPI_REAL
#else
integer , parameter :: rp =3D selected_real_kind(15)
integer , parameter :: mpi_type =3D MPI_REAL8
#endif
!Define some physical constants to use throughout the simulation
real(rp), parameter :: pi =3D 3.14159265358979323846264338327_rp !Pi
real(rp), parameter :: grav =3D 9.8_rp !Gravitational acceleration (m / s^2=
)
real(rp), parameter :: cp =3D 1004._rp !Specific heat of dry air at constan=
t pressure
real(rp), parameter :: cv =3D 717._rp !Specific heat of dry air at constant=
 volume
real(rp), parameter :: rd =3D 287._rp !Dry air constant for equation of sta=
te (P=3Drho*rd*T)
real(rp), parameter :: p0 =3D 1.e5_rp !Standard pressure at the surface in =
Pascals
real(rp), parameter :: C0 =3D 27.5629410929725921310572974482_rp !Constant =
to translate potential temperature into pressure (P=3DC0*(rho*theta)**gamma=
)
real(rp), parameter :: gamma =3D 1.40027894002789400278940027894_rp !gamma=
=3Dcp/Rd
!Define domain and stability-related constants
real(rp), parameter :: xlen =3D 2.e4_rp !Length of the domain in the x-dire=
ction (meters)
real(rp), parameter :: zlen =3D 1.e4_rp !Length of the domain in the z-dire=
ction (meters)
real(rp), parameter :: hv_beta =3D 0.05_rp !How strong to diffuse the solut=
ion: hv_beta \in [0:1]
real(rp), parameter :: cfl =3D 1.50_rp !"Courant, Friedrichs, Lewy" number =
(for numerical stability)
real(rp), parameter :: max_speed =3D 450 !Assumed maximum wave speed during=
 the simulation (speed of sound + speed of wind) (meter / sec)
integer , parameter :: hs =3D 2 !"Halo" size: number of cells beyond the MP=
I tasks's domain needed for a full "stencil" of information for reconstruct=
ion
integer , parameter :: sten_size =3D 4 !Size of the stencil used for interp=
olation

!Parameters for indexing and flags
integer , parameter :: NUM_VARS =3D 4 !Number of fluid state variables
integer , parameter :: ID_DENS =3D 1 !index for density ("rho")
integer , parameter :: ID_UMOM =3D 2 !index for momentum in the x-direction=
 ("rho * u")
integer , parameter :: ID_WMOM =3D 3 !index for momentum in the z-direction=
 ("rho * w")
integer , parameter :: ID_RHOT =3D 4 !index for density * potential tempera=
ture ("rho * theta")
integer , parameter :: DIR_X =3D 1 !Integer constant to express that this o=
peration is in the x-direction
integer , parameter :: DIR_Z =3D 2 !Integer constant to express that this o=
peration is in the z-direction
integer , parameter :: DATA_SPEC_COLLISION =3D 1
integer , parameter :: DATA_SPEC_THERMAL =3D 2
integer , parameter :: DATA_SPEC_GRAVITY_WAVES =3D 3
integer , parameter :: DATA_SPEC_DENSITY_CURRENT =3D 5
integer , parameter :: DATA_SPEC_INJECTION =3D 6

!Gauss-Legendre quadrature points and weights on the domain [0:1]
integer , parameter :: nqpoints =3D 3
real(rp), parameter :: qpoints (nqpoints) =3D (/ 0.112701665379258311482073=
460022E0_rp , 0.500000000000000000000000000000E0_rp , 0.8872983346207416885=
17926539980E0_rp /)
real(rp), parameter :: qweights(nqpoints) =3D (/ 0.277777777777777777777777=
777779E0_rp , 0.444444444444444444444444444444E0_rp , 0.2777777777777777777=
77777777779E0_rp /)

